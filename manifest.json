{
  "name": "RES Omnibox CommandLine",
  "short_name": "RES Omniline",
  "description" : "To use, type 'res' to trigger a psudo RES commandline in the omnibox.",
  "version": "1.2.0",
  "manifest_version": 2,
  "background": {
	  "persistent": false,
	  "scripts": ["background.js"]
  },
  "icons": {
	  "16": "icons2/icon16.png",
	  "48": "icons2/icon48.png",
	  "64": "icons2/icon64.png",
	  "128": "icons2/icon128.png",
	  "1024": "icons2/icon1024.png"
  },
  "omnibox": { "keyword" : "res" },
  "permissions": ["tabs", "storage"]
}